algoritmo "Lista 1.1 - Básico - Exercicio 1 A"

var
n, x, y, custo : real

inicio
escreva ("Digite a quantidade de canetas: ")
leia (n)
escreva ("Digite o valor da nota: ")
leia (x)
escreva ("Digite quanto obteve de troco: ")
leia (y)

custo <- (x-y)/n

escreval ("O custo da caneta é de: ",custo)

fimalgoritmo